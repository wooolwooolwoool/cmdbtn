{
  "name": "CommandRunnerbutton",
  "displayName": "Command Runner Button",
  "description": "Command Runner",
  "version": "0.0.5",
  "icon": "media/icon.png",
  "publisher": "woool",
  "repository": {
    "type": "git",
    "url": "https://github.com/wooolwooolwoool/cmdbtn.git"
  },
  "license": "GPL-2.0-only",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:commandSidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "commandSidebar",
          "title": "Command Runner",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "commandSidebar": [
        {
          "type": "webview",
          "id": "commandSidebar",
          "name": "Command Runner"
        }
      ]
    },
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.2",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/mocha": "^10.0.10",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/vsce": "^3.3.0",
    "vsce": "^2.15.0"
  }
}
